@model IEnumerable<Proyecto_AirBnb.Models.Reserva>


@if (Model.Count() == 0)
{
    <div class="panel panel-default">
        <h2>No tienes reservas aun</h2>
    </div>
}
else
{
    <table class="table">
        <tr>
            <th>
                Titulo
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_Huesped)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_Anuncio)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model) {
    <tr>
        <td>
            Ya veresmos como lo hago
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id_Huesped)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id_Anuncio)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id_Reserva }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id_Reserva }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id_Reserva })
        </td>
    </tr>
}

    </table>
}
